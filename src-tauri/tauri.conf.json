{
  "build": {
    "beforeBuildCommand": "rm -rf dist && cd ./graphx-client && npm i && npm run build && mv dist ../dist",
    "beforeDevCommand": "cd ./graphx-client && npm run dev",
    "devPath": "http://localhost:5173"
  },
  "package": {
    "productName": "graphx-tauri",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "graphx.com",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "graphx",
        "width": 800
      }
    ]
  }
}
